<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ActivityPueblos">

    <Button
        android:id="@+id/logoBurjassot"
        style="@style/boton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="55dp"
        android:layout_marginTop="184dp"
        android:layout_marginEnd="55dp"
        android:layout_marginBottom="34dp"
        android:background="@drawable/boton_redondo"
        android:drawableStart="@drawable/btnmistic"
        android:gravity="center"
        android:text="Burjassot"
        app:layout_constraintBottom_toTopOf="@+id/logoGodella"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/logoGodella"
        style="@style/boton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="55dp"
        android:layout_marginEnd="55dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/boton_redondo"
        android:drawableStart="@drawable/botonvalor"
        android:gravity="center"
        android:paddingStart="10dp"
        android:text="Godella"
        app:layout_constraintBottom_toTopOf="@+id/logoBenimamet"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logoBurjassot" />

    <Button
        android:id="@+id/logoBenimamet"
        style="@style/boton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="55dp"
        android:layout_marginEnd="55dp"
        android:layout_marginBottom="151dp"
        android:background="@drawable/boton_redondo"
        android:drawableStart="@drawable/botoninstinto"
        android:gravity="center"
        android:text="Benimamet"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logoGodella" />


</androidx.constraintlayout.widget.ConstraintLayout>